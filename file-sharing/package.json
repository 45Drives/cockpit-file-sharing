    "eslint": "^9.2.0",
    "eslint-plugin-vue": "^9.26.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "postcss-modules": "^6.0.0",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
