    "@heroicons/vue": "^2.0.18",
    "monet": "^0.9.3",
    "neverthrow": "^6.2.1",
    "vue": "^3.5",
    "vue-router": "^4.3.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@fontsource/red-hat-text": "^5.0.18",
    "@rushstack/eslint-patch": "^1.10.2",
